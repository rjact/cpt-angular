{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
	{
		"name": "sequences",
		"urls":[
			"/getSequences?ParentId=*"
		],
		"cacheConfig": {
			"maxSize": 100000,
			"maxAge": "2d",
			"timeout": "5s",
			"strategy": "freshness"
		}
	},	{
		"name": "cpt-codes",
		"urls":[
			"/getCptsForSequence?SequenceId=*"
		],
		"cacheConfig": {
			"maxSize": 100000,
			"maxAge": "2d",
			"timeout": "5s",
			"strategy": "freshness"
		}
	}
  ]
}